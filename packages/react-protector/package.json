{
  "name": "react-protector",
  "version": "1.0.0",
  "description": "Declarative react and react-router auth helpers",
  "scripts": {
    "build": "node ./tools/build.js",
    "watch": "babel ./modules -d . --ignore __tests__ --watch",
    "prepublishOnly": "node ./tools/build.js",
    "clean": "git clean -fdX .",
    "lint": "eslint modules",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "github.com/iamandrewluca/react-protector"
  },
  "keywords": [
    "react",
    "router",
    "route",
    "routing",
    "authentication"
  ],
  "author": "Andrew Luca <thendrluca@gmail.com> (https://iamandrewluca.com)",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16",
    "react-router": ">=4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "^4.16.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.6.0",
    "gzip-size": "^4.1.0",
    "jest": "^22.1.4",
    "pretty-bytes": "^4.0.2",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "rollup": "^0.50.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "files": [
    "es",
    "umd",
    "index.js",
    "normalizeRoles.js",
    "Protected.js",
    "ProtectedRoute.js",
    "ProtectedRoutes.js",
    "ProtectionContext.js",
    "Public.js",
    "PublicRoute.js",
    "RestrictedRoute.js"
  ],
  "main": "index.js",
  "module": "es/index.js",
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "dependencies": {
    "create-react-context": "^0.2.1",
    "lodash.intersection": "^4.4.0",
    "prop-types": "^15.6.1"
  }
}
